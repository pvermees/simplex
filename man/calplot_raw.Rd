## The function is currently defined as
function (dat, i = NULL, c64 = NULL, ...) 
{
    vars <- reshuffle(dat)
    nt <- nrow(dat[[1]]$time)
    ns <- length(dat)
    if (is.null(i)) 
        ii <- 1:ns
    else ii <- i
    matplot(vars$X[, ii], vars$Y[, ii], type = "l", ...)
    text(vars$X[, ii], vars$Y[, ii], labels = 1:nt)
  }
}
