settings <- function(setting,value){
    out <- list()
    out$ions <- list()
    out$ions[['IGG-zircon']] <- c('Zr90','Zr92','200.5','Zr94',
                                  'Pb204','Pb206','Pb207','Pb208',
                                  'U238','ThO2','UO2','270.1')
    out$ions[['IGG-monazite']] <- c('La139','202.5','Pb204','Pb206',
                                    'Pb207','Pb208','Th232','U238',
                                    'Th232','UO2')
    out$ions[['GA-zircon']] <- c('Zr2O','Pb204','bkg','Pb206','Pb207',
                                 'Pb208','U238','ThO','UO','UO2')
}
